#!/usr/bin/env zsh

SCRIPT_DIR=${0:a:h}

for pair in 3,1 9,1 15,1 15,3 15,5 15,7 15,9 21,1 27,1; do
    IFS=',' read -r masters replicas <<< $pair

    ${SCRIPT_DIR}/../../patch-benchmark/scripts/notify "Start Masters: $masters Replicas: $replicas"
    execute $masters $replicas
    ${SCRIPT_DIR}/../../patch-benchmark/scripts/notify "End Masters: $masters Replicas: $replicas"
done

${SCRIPT_DIR}/../../collect-results ${experiment_name}

