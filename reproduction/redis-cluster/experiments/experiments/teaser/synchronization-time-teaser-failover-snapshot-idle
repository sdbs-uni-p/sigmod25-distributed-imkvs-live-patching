#!/usr/bin/env zsh
set -euv

SCRIPT_DIR=${0:a:h}

experiment_name="synchronization-time-teaser-failover-snapshot-idle"
execute () {
    data=$1
    
    echo "Data: $data"

    cd ${SCRIPT_DIR}/../../
    pipenv run python patch-benchmark --config config-cluster-failover-synchronization-idle.yaml --overwrite redis_cluster.config="cluster-configs/cluster-3-1.yaml" data.max_memory_usage_gb=$data data._iterations="None" failover.snapshot=True
}

source ${SCRIPT_DIR}/util

