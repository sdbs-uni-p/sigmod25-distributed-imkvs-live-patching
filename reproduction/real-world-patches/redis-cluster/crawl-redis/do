#!/usr/bin/env zsh

SCRIPT_DIR=${0:a:h}

cd ${SCRIPT_DIR}
pipenv run ./crawl.py

cd ${SCRIPT_DIR}/crawl-redis
./crawl ../crawled-commits.txt

cd ${SCRIPT_DIR}

mkdir -p ${SCRIPT_DIR}/../../../data/output
./analyze-real-world-success-patch-list ${SCRIPT_DIR}/crawl-redis/output/patches > ${SCRIPT_DIR}/../../../data/output/real-world-patches-redis.txt

